{
  "camelcase": true,
  "curly": true,
  "eqeqeq": true,
  "eqnull": true,
  "freeze": true,
  "funcscope": true,
  "globals": {
    "$"        : false,
    "jQuery"   : false,
    "scope"   : false,
    "elements"   : false,
    "angular"  : false,
    "document" : false,
    "Firebase" : false,
    "FB"       : false,
    "moment"   : false,
    "window"   : false
  },
  "indent": 2,
  "maxdepth": 3,
  "maxlen": 80,
  "maxstatements": 50,
  "newcap": false,
  "node": true,
  "quotmark": "single",
  "strict": true,
  "undef": true,
  "unused": true
}
